{
  "name": "wing-night",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "pnpm --filter @wingnight/shared build && pnpm -r --parallel --filter @wingnight/shared --filter @wingnight/server --filter @wingnight/client dev",
    "build": "pnpm -r --if-present build",
    "typecheck": "pnpm -r --if-present typecheck",
    "test": "pnpm -r --if-present test && pnpm test:lint-rules",
    "test:lint-rules": "node --test tools/eslint-plugin-wingnight/tests/index.test.mjs",
    "lint": "pnpm -r --if-present lint"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "typescript-eslint": "^8.21.0"
  }
}
